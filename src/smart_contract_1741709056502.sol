```solidity
/**
 * @title Decentralized Autonomous Art Collective (DAAC) Smart Contract
 * @author Gemini AI (Conceptual - Demonstrative Purposes Only)
 * @dev A smart contract for a Decentralized Autonomous Art Collective (DAAC) that enables artists to submit art,
 * members to curate and vote on submissions, manage exhibitions, conduct auctions, and govern the collective's treasury and rules.
 * This contract is designed to be creative and incorporate advanced concepts, avoiding duplication of common open-source contracts.
 *
 * **Outline & Function Summary:**
 *
 * **1. Art Submission & Management:**
 *    - `submitArt(string _metadataURI, string _title, string _description)`: Allows artists to submit their art with associated metadata.
 *    - `updateArtMetadata(uint256 _artId, string _newMetadataURI, string _newTitle, string _newDescription)`: Artists can update metadata of their submitted art (before curation).
 *    - `removeArtSubmission(uint256 _artId)`: Artists can remove their art submission before curation.
 *    - `getArtSubmission(uint256 _artId)`: Retrieve details of a specific art submission.
 *    - `getAllArtSubmissions()`: Get a list of all art submissions (for admin/curation purposes).
 *
 * **2. Curation & Voting:**
 *    - `startCurationRound(string _roundDescription, uint256 _durationDays)`: Starts a new curation round for art submissions.
 *    - `voteOnArt(uint256 _artId, bool _approve)`: Members can vote to approve or reject art submissions during a curation round.
 *    - `endCurationRound()`: Ends the current curation round and processes voting results.
 *    - `getCurationRoundStatus()`: Get the status of the current curation round (active/inactive).
 *    - `getCurationRoundResults(uint256 _roundId)`: Retrieve the results of a specific curation round.
 *
 * **3. Exhibition Management:**
 *    - `createExhibition(string _exhibitionName, string _exhibitionDescription, uint256 _startDate, uint256 _endDate)`: Create a new art exhibition.
 *    - `addArtToExhibition(uint256 _exhibitionId, uint256 _artId)`: Add curated art to a specific exhibition.
 *    - `removeArtFromExhibition(uint256 _exhibitionId, uint256 _artId)`: Remove art from an exhibition.
 *    - `getExhibitionDetails(uint256 _exhibitionId)`: Retrieve details of a specific exhibition.
 *    - `getActiveExhibitions()`: Get a list of currently active exhibitions.
 *
 * **4. Auction & Sales:**
 *    - `startAuction(uint256 _artId, uint256 _startingBid, uint256 _auctionDurationHours)`: Start an auction for a curated artwork.
 *    - `bidOnAuction(uint256 _auctionId)`: Place a bid on an active auction.
 *    - `endAuction(uint256 _auctionId)`: End an auction and settle the sale to the highest bidder.
 *    - `setArtSalePrice(uint256 _artId, uint256 _price)`: Set a fixed sale price for curated art (alternative to auction).
 *    - `purchaseArt(uint256 _artId)`: Purchase art at a fixed sale price.
 *
 * **5. Membership & Governance:**
 *    - `joinCollective()`: Allow users to request membership to the DAAC.
 *    - `approveMembership(address _memberAddress)`: Admin/Curators can approve membership requests.
 *    - `removeMembership(address _memberAddress)`: Admin can remove a member from the collective.
 *    - `proposeRuleChange(string _proposalDescription, string _ruleDetails)`: Members can propose changes to the DAAC rules.
 *    - `voteOnRuleChange(uint256 _proposalId, bool _support)`: Members can vote on rule change proposals.
 *    - `executeRuleChange(uint256 _proposalId)`: Admin executes approved rule changes.
 *
 * **6. Treasury Management (Conceptual):**
 *    - `depositToTreasury()`: Allow revenue from sales/auctions to be deposited into the treasury (implicit in sales/auction functions).
 *    - `proposeTreasurySpending(string _spendingDescription, address _recipient, uint256 _amount)`: Members can propose treasury spending.
 *    - `voteOnTreasurySpending(uint256 _proposalId, bool _support)`: Members can vote on treasury spending proposals.
 *    - `executeTreasurySpending(uint256 _proposalId)`: Admin executes approved treasury spending.
 *
 * **7. Utility & Admin Functions:**
 *    - `pauseContract()`: Admin function to pause critical contract functionalities in emergencies.
 *    - `unpauseContract()`: Admin function to resume contract functionalities.
 *    - `setAdmin(address _newAdmin)`: Change the contract administrator.
 *    - `getContractBalance()`: Get the contract's current Ether balance (for transparency).
 */

pragma solidity ^0.8.0;

contract DecentralizedAutonomousArtCollective {

    // --- Structs & Enums ---

    struct ArtSubmission {
        uint256 id;
        address artist;
        string metadataURI;
        string title;
        string description;
        uint256 submissionTimestamp;
        bool isCurated;
        bool isRejected;
    }

    struct CurationRound {
        uint256 id;
        string description;
        uint256 startTime;
        uint256 endTime;
        bool isActive;
        mapping(uint256 => mapping(address => bool)) votes; // artId => voter => vote (true=approve, false=reject)
        mapping(uint256 => uint256) approvalVotes; // artId => count of approval votes
        mapping(uint256 => uint256) rejectionVotes; // artId => count of rejection votes
        mapping(uint256 => bool) curationResults; // artId => curation outcome (true=curated, false=rejected)
    }

    struct Exhibition {
        uint256 id;
        string name;
        string description;
        uint256 startDate;
        uint256 endDate;
        bool isActive;
        uint256[] artIds;
    }

    struct Auction {
        uint256 id;
        uint256 artId;
        uint256 startTime;
        uint256 endTime;
        uint256 startingBid;
        address highestBidder;
        uint256 highestBid;
        bool isActive;
        bool isSettled;
    }

    struct RuleChangeProposal {
        uint256 id;
        string description;
        string ruleDetails;
        uint256 proposalTime;
        bool isActive;
        mapping(address => bool) votes; // voter => vote (true=support, false=oppose)
        uint256 supportVotes;
        uint256 opposeVotes;
        bool isExecuted;
    }

    struct TreasurySpendingProposal {
        uint256 id;
        string description;
        address recipient;
        uint256 amount;
        uint256 proposalTime;
        bool isActive;
        mapping(address => bool) votes; // voter => vote (true=support, false=oppose)
        uint256 supportVotes;
        uint256 opposeVotes;
        bool isExecuted;
    }

    enum MembershipStatus { PENDING, APPROVED, REJECTED }

    // --- State Variables ---

    address public admin;
    bool public contractPaused;

    uint256 public artSubmissionCounter;
    mapping(uint256 => ArtSubmission) public artSubmissions;
    uint256[] public allArtSubmissionIds; // For efficient iteration

    uint256 public curationRoundCounter;
    mapping(uint256 => CurationRound) public curationRounds;
    uint256 public activeCurationRoundId;

    uint256 public exhibitionCounter;
    mapping(uint256 => Exhibition) public exhibitions;
    uint256[] public activeExhibitionIds; // Keep track of active exhibitions

    uint256 public auctionCounter;
    mapping(uint256 => Auction) public auctions;
    mapping(uint256 => uint256) public artSalePrices; // artId => sale price for fixed price sales

    mapping(address => MembershipStatus) public membershipStatus;
    address[] public members;

    uint256 public ruleChangeProposalCounter;
    mapping(uint256 => RuleChangeProposal) public ruleChangeProposals;

    uint256 public treasurySpendingProposalCounter;
    mapping(uint256 => TreasurySpendingProposal) public treasurySpendingProposals;

    uint256 public membershipFee = 0.1 ether; // Example membership fee

    uint256 public curationVoteDurationDays = 7; // Default curation vote duration
    uint256 public ruleChangeVoteDurationDays = 14; // Default rule change vote duration
    uint256 public treasurySpendingVoteDurationDays = 10; // Default treasury spending vote duration

    uint256 public curationApprovalThresholdPercentage = 50; // Percentage of votes needed for curation approval
    uint256 public ruleChangeApprovalThresholdPercentage = 60; // Percentage for rule change approval
    uint256 public treasurySpendingApprovalThresholdPercentage = 70; // Percentage for treasury spending approval


    // --- Events ---

    event ArtSubmitted(uint256 artId, address artist, string metadataURI, string title);
    event ArtMetadataUpdated(uint256 artId, string newMetadataURI, string newTitle, string newDescription);
    event ArtSubmissionRemoved(uint256 artId, address artist);
    event CurationRoundStarted(uint256 roundId, string description, uint256 startTime, uint256 endTime);
    event ArtVotedOn(uint256 artId, address voter, bool vote);
    event CurationRoundEnded(uint256 roundId);
    event ArtCurated(uint256 artId);
    event ArtRejected(uint256 artId);
    event ExhibitionCreated(uint256 exhibitionId, string name, uint256 startDate, uint256 endDate);
    event ArtAddedToExhibition(uint256 exhibitionId, uint256 artId);
    event ArtRemovedFromExhibition(uint256 exhibitionId, uint256 artId);
    event AuctionStarted(uint256 auctionId, uint256 artId, uint256 startingBid, uint256 endTime);
    event BidPlaced(uint256 auctionId, address bidder, uint256 bidAmount);
    event AuctionEnded(uint256 auctionId, uint256 artId, address winner, uint256 winningBid);
    event ArtSold(uint256 artId, address buyer, uint256 price);
    event MembershipRequested(address memberAddress);
    event MembershipApproved(address memberAddress);
    event MembershipRemoved(address memberAddress);
    event RuleChangeProposed(uint256 proposalId, string description, string ruleDetails);
    event RuleChangeVotedOn(uint256 proposalId, address voter, bool support);
    event RuleChangeExecuted(uint256 proposalId);
    event TreasurySpendingProposed(uint256 proposalId, string description, address recipient, uint256 amount);
    event TreasurySpendingVotedOn(uint256 proposalId, address voter, bool support);
    event TreasurySpendingExecuted(uint256 proposalId, address recipient, uint256 amount);
    event ContractPaused(address admin);
    event ContractUnpaused(address admin);
    event AdminChanged(address oldAdmin, address newAdmin);


    // --- Modifiers ---

    modifier onlyAdmin() {
        require(msg.sender == admin, "Only admin can call this function.");
        _;
    }

    modifier onlyMember() {
        require(membershipStatus[msg.sender] == MembershipStatus.APPROVED, "Only approved members can call this function.");
        _;
    }

    modifier contractNotPaused() {
        require(!contractPaused, "Contract is paused.");
        _;
    }

    modifier curationRoundActive() {
        require(curationRounds[activeCurationRoundId].isActive, "No active curation round.");
        _;
    }

    modifier curationRoundNotActive() {
        require(!curationRounds[activeCurationRoundId].isActive, "Curation round is already active.");
        _;
    }

    modifier auctionActive(uint256 _auctionId) {
        require(auctions[_auctionId].isActive, "Auction is not active.");
        _;
    }

    modifier auctionNotSettled(uint256 _auctionId) {
        require(!auctions[_auctionId].isSettled, "Auction is already settled.");
        _;
    }

    modifier proposalActive(mapping(uint256 => RuleChangeProposal) storage _proposals, uint256 _proposalId) {
        require(_proposals[_proposalId].isActive, "Proposal is not active.");
        _;
    }

    modifier proposalNotExecuted(mapping(uint256 => RuleChangeProposal) storage _proposals, uint256 _proposalId) {
        require(!_proposals[_proposalId].isExecuted, "Proposal is already executed.");
        _;
    }


    // --- Constructor ---

    constructor() payable {
        admin = msg.sender;
        contractPaused = false;
        artSubmissionCounter = 0;
        curationRoundCounter = 0;
        exhibitionCounter = 0;
        auctionCounter = 0;
        activeCurationRoundId = 0; // No active round initially
    }


    // --- 1. Art Submission & Management ---

    /// @notice Allows artists to submit their art with associated metadata.
    /// @param _metadataURI URI pointing to the art's metadata (e.g., IPFS).
    /// @param _title Title of the artwork.
    /// @param _description Description of the artwork.
    function submitArt(string memory _metadataURI, string memory _title, string memory _description) external contractNotPaused {
        artSubmissionCounter++;
        ArtSubmission storage newSubmission = artSubmissions[artSubmissionCounter];
        newSubmission.id = artSubmissionCounter;
        newSubmission.artist = msg.sender;
        newSubmission.metadataURI = _metadataURI;
        newSubmission.title = _title;
        newSubmission.description = _description;
        newSubmission.submissionTimestamp = block.timestamp;
        newSubmission.isCurated = false;
        newSubmission.isRejected = false;
        allArtSubmissionIds.push(artSubmissionCounter);

        emit ArtSubmitted(artSubmissionCounter, msg.sender, _metadataURI, _title);
    }

    /// @notice Artists can update metadata of their submitted art before curation.
    /// @param _artId ID of the art submission.
    /// @param _newMetadataURI New URI pointing to the art's metadata.
    /// @param _newTitle New title of the artwork.
    /// @param _newDescription New description of the artwork.
    function updateArtMetadata(uint256 _artId, string memory _newMetadataURI, string memory _newTitle, string memory _newDescription) external contractNotPaused {
        ArtSubmission storage submission = artSubmissions[_artId];
        require(submission.artist == msg.sender, "Only the artist can update metadata.");
        require(!submission.isCurated && !submission.isRejected, "Cannot update metadata of curated or rejected art.");

        submission.metadataURI = _newMetadataURI;
        submission.title = _newTitle;
        submission.description = _newDescription;

        emit ArtMetadataUpdated(_artId, _newMetadataURI, _newTitle, _newDescription);
    }

    /// @notice Artists can remove their art submission before curation.
    /// @param _artId ID of the art submission to remove.
    function removeArtSubmission(uint256 _artId) external contractNotPaused {
        ArtSubmission storage submission = artSubmissions[_artId];
        require(submission.artist == msg.sender, "Only the artist can remove their submission.");
        require(!submission.isCurated && !submission.isRejected, "Cannot remove curated or rejected art.");

        submission.artist = address(0); // Mark as removed, keep ID for record. Can optimize by removing from `allArtSubmissionIds` if needed.
        emit ArtSubmissionRemoved(_artId, msg.sender);
    }

    /// @notice Retrieve details of a specific art submission.
    /// @param _artId ID of the art submission.
    /// @return ArtSubmission struct containing details.
    function getArtSubmission(uint256 _artId) external view returns (ArtSubmission memory) {
        return artSubmissions[_artId];
    }

    /// @notice Get a list of all art submissions (for admin/curation purposes).
    /// @return Array of art submission IDs.
    function getAllArtSubmissions() external view onlyAdmin returns (uint256[] memory) {
        return allArtSubmissionIds;
    }


    // --- 2. Curation & Voting ---

    /// @notice Starts a new curation round for art submissions.
    /// @param _roundDescription Description of the curation round.
    /// @param _durationDays Duration of the curation round in days.
    function startCurationRound(string memory _roundDescription, uint256 _durationDays) external onlyAdmin contractNotPaused curationRoundNotActive {
        curationRoundCounter++;
        activeCurationRoundId = curationRoundCounter;
        CurationRound storage newRound = curationRounds[activeCurationRoundId];
        newRound.id = activeCurationRoundId;
        newRound.description = _roundDescription;
        newRound.startTime = block.timestamp;
        newRound.endTime = block.timestamp + (_durationDays * 1 days); // days to seconds
        newRound.isActive = true;

        emit CurationRoundStarted(activeCurationRoundId, _roundDescription, newRound.startTime, newRound.endTime);
    }

    /// @notice Members can vote to approve or reject art submissions during a curation round.
    /// @param _artId ID of the art submission to vote on.
    /// @param _approve True to approve, false to reject.
    function voteOnArt(uint256 _artId, bool _approve) external onlyMember contractNotPaused curationRoundActive {
        CurationRound storage currentRound = curationRounds[activeCurationRoundId];
        require(!currentRound.votes[_artId][msg.sender], "Member has already voted on this art.");
        require(!artSubmissions[_artId].isCurated && !artSubmissions[_artId].isRejected, "Cannot vote on already curated or rejected art.");

        currentRound.votes[_artId][msg.sender] = _approve;
        if (_approve) {
            currentRound.approvalVotes[_artId]++;
        } else {
            currentRound.rejectionVotes[_artId]++;
        }

        emit ArtVotedOn(_artId, msg.sender, _approve);
    }

    /// @notice Ends the current curation round and processes voting results.
    function endCurationRound() external onlyAdmin contractNotPaused curationRoundActive {
        CurationRound storage currentRound = curationRounds[activeCurationRoundId];
        require(block.timestamp >= currentRound.endTime, "Curation round is not yet ended.");

        currentRound.isActive = false;
        uint256 totalMembers = members.length;

        for (uint256 i = 0; i < allArtSubmissionIds.length; i++) {
            uint256 artId = allArtSubmissionIds[i];
            if (artSubmissions[artId].artist != address(0) && !artSubmissions[artId].isCurated && !artSubmissions[artId].isRejected) { // Process only active, not curated/rejected submissions
                uint256 totalVotesForArt = currentRound.approvalVotes[artId] + currentRound.rejectionVotes[artId];
                if (totalVotesForArt > 0) { // Ensure there were votes for this artwork
                    uint256 approvalPercentage = (currentRound.approvalVotes[artId] * 100) / totalMembers; // Calculate percentage based on total members
                    if (approvalPercentage >= curationApprovalThresholdPercentage) {
                        artSubmissions[artId].isCurated = true;
                        emit ArtCurated(artId);
                    } else {
                        artSubmissions[artId].isRejected = true;
                        emit ArtRejected(artId);
                    }
                } else {
                    // If no votes, maybe default to rejected or handle differently based on collective policy
                    artSubmissions[artId].isRejected = true; // Default to rejected if no votes
                    emit ArtRejected(artId); // Or create a new event: ArtNoVotesReceived(artId)
                }
            }
        }

        emit CurationRoundEnded(activeCurationRoundId);
        activeCurationRoundId = 0; // Reset active round
    }

    /// @notice Get the status of the current curation round (active/inactive).
    /// @return isActive - boolean indicating if a curation round is currently active.
    function getCurationRoundStatus() external view returns (bool isActive, uint256 roundId, uint256 endTime) {
        return (curationRounds[activeCurationRoundId].isActive, activeCurationRoundId, curationRounds[activeCurationRoundId].endTime);
    }

    /// @notice Retrieve the results of a specific curation round.
    /// @param _roundId ID of the curation round.
    /// @return roundDetails - CurationRound struct containing details of the round.
    function getCurationRoundResults(uint256 _roundId) external view onlyAdmin returns (CurationRound memory roundDetails) {
        return curationRounds[_roundId];
    }


    // --- 3. Exhibition Management ---

    /// @notice Create a new art exhibition.
    /// @param _exhibitionName Name of the exhibition.
    /// @param _exhibitionDescription Description of the exhibition.
    /// @param _startDate Unix timestamp for the exhibition start date.
    /// @param _endDate Unix timestamp for the exhibition end date.
    function createExhibition(string memory _exhibitionName, string memory _exhibitionDescription, uint256 _startDate, uint256 _endDate) external onlyAdmin contractNotPaused {
        exhibitionCounter++;
        Exhibition storage newExhibition = exhibitions[exhibitionCounter];
        newExhibition.id = exhibitionCounter;
        newExhibition.name = _exhibitionName;
        newExhibition.description = _exhibitionDescription;
        newExhibition.startDate = _startDate;
        newExhibition.endDate = _endDate;
        newExhibition.isActive = true;
        newExhibition.artIds = new uint256[](0); // Initialize with empty art list

        activeExhibitionIds.push(exhibitionCounter); // Add to active exhibitions list

        emit ExhibitionCreated(exhibitionCounter, _exhibitionName, _startDate, _endDate);
    }

    /// @notice Add curated art to a specific exhibition.
    /// @param _exhibitionId ID of the exhibition.
    /// @param _artId ID of the curated artwork to add.
    function addArtToExhibition(uint256 _exhibitionId, uint256 _artId) external onlyAdmin contractNotPaused {
        Exhibition storage exhibition = exhibitions[_exhibitionId];
        require(exhibition.isActive, "Exhibition is not active.");
        require(artSubmissions[_artId].isCurated, "Art is not yet curated.");

        exhibition.artIds.push(_artId);
        emit ArtAddedToExhibition(_exhibitionId, _artId);
    }

    /// @notice Remove art from an exhibition.
    /// @param _exhibitionId ID of the exhibition.
    /// @param _artId ID of the artwork to remove.
    function removeArtFromExhibition(uint256 _exhibitionId, uint256 _artId) external onlyAdmin contractNotPaused {
        Exhibition storage exhibition = exhibitions[_exhibitionId];
        require(exhibition.isActive, "Exhibition is not active.");

        uint256[] storage artList = exhibition.artIds;
        for (uint256 i = 0; i < artList.length; i++) {
            if (artList[i] == _artId) {
                // Remove element by shifting elements after it
                for (uint256 j = i; j < artList.length - 1; j++) {
                    artList[j] = artList[j+1];
                }
                artList.pop();
                emit ArtRemovedFromExhibition(_exhibitionId, _artId);
                return;
            }
        }
        revert("Art not found in exhibition.");
    }

    /// @notice Retrieve details of a specific exhibition.
    /// @param _exhibitionId ID of the exhibition.
    /// @return Exhibition struct containing details.
    function getExhibitionDetails(uint256 _exhibitionId) external view returns (Exhibition memory) {
        return exhibitions[_exhibitionId];
    }

    /// @notice Get a list of currently active exhibitions.
    /// @return Array of active exhibition IDs.
    function getActiveExhibitions() external view returns (uint256[] memory) {
        return activeExhibitionIds;
    }


    // --- 4. Auction & Sales ---

    /// @notice Start an auction for a curated artwork.
    /// @param _artId ID of the curated artwork.
    /// @param _startingBid Starting bid amount in Wei.
    /// @param _auctionDurationHours Duration of the auction in hours.
    function startAuction(uint256 _artId, uint256 _startingBid, uint256 _auctionDurationHours) external onlyAdmin contractNotPaused {
        require(artSubmissions[_artId].isCurated, "Only curated art can be auctioned.");
        require(auctions[auctionCounter].isActive == false || auctions[auctionCounter].id == 0, "An auction is already in progress. Please end it first or increment auctionCounter manually if needed."); // Basic check to prevent overlapping auctions on same counter - consider better auction ID management.

        auctionCounter++;
        Auction storage newAuction = auctions[auctionCounter];
        newAuction.id = auctionCounter;
        newAuction.artId = _artId;
        newAuction.startTime = block.timestamp;
        newAuction.endTime = block.timestamp + (_auctionDurationHours * 1 hours);
        newAuction.startingBid = _startingBid;
        newAuction.highestBid = _startingBid; // Initial highest bid is starting bid
        newAuction.highestBidder = address(this); // Placeholder, no bidder yet
        newAuction.isActive = true;
        newAuction.isSettled = false;

        emit AuctionStarted(auctionCounter, _artId, _startingBid, newAuction.endTime);
    }

    /// @notice Place a bid on an active auction.
    /// @param _auctionId ID of the auction.
    function bidOnAuction(uint256 _auctionId) external payable contractNotPaused auctionActive(_auctionId) auctionNotSettled(_auctionId) {
        Auction storage auction = auctions[_auctionId];
        require(msg.value > auction.highestBid, "Bid must be higher than the current highest bid.");

        // Refund previous highest bidder (if any, and if not the contract itself which is placeholder)
        if (auction.highestBidder != address(this)) {
            payable(auction.highestBidder).transfer(auction.highestBid);
        }

        auction.highestBidder = msg.sender;
        auction.highestBid = msg.value;

        emit BidPlaced(_auctionId, msg.sender, msg.value);
    }

    /// @notice End an auction and settle the sale to the highest bidder.
    /// @param _auctionId ID of the auction to end.
    function endAuction(uint256 _auctionId) external onlyAdmin contractNotPaused auctionActive(_auctionId) auctionNotSettled(_auctionId) {
        Auction storage auction = auctions[_auctionId];
        require(block.timestamp >= auction.endTime, "Auction is not yet ended.");

        auction.isActive = false;
        auction.isSettled = true;

        if (auction.highestBidder != address(this)) { // If there was a bidder (not just starting bid)
            // Transfer funds to the collective treasury (or artist if configured)
            payable(admin).transfer(auction.highestBid); // For simplicity, sending to admin for now - can be treasury management logic later.
            emit AuctionEnded(_auctionId, auction.artId, auction.highestBidder, auction.highestBid);
            emit ArtSold(auction.artId, auction.highestBidder, auction.highestBid);
        } else {
            // No bids higher than starting bid - auction ends without sale.
            emit AuctionEnded(_auctionId, auction.artId, address(0), 0); // Winner address 0 indicates no sale.
        }
    }

    /// @notice Set a fixed sale price for curated art (alternative to auction).
    /// @param _artId ID of the curated artwork.
    /// @param _price Sale price in Wei.
    function setArtSalePrice(uint256 _artId, uint256 _price) external onlyAdmin contractNotPaused {
        require(artSubmissions[_artId].isCurated, "Only curated art can be put on sale.");
        artSalePrices[_artId] = _price;
    }

    /// @notice Purchase art at a fixed sale price.
    /// @param _artId ID of the artwork to purchase.
    function purchaseArt(uint256 _artId) external payable contractNotPaused {
        require(artSalePrices[_artId] > 0, "Art is not for fixed price sale.");
        require(msg.value >= artSalePrices[_artId], "Insufficient payment.");

        uint256 price = artSalePrices[_artId];
        delete artSalePrices[_artId]; // Remove from sale after purchase (one-time sale for now).

        // Transfer funds to the collective treasury (or artist if configured)
        payable(admin).transfer(price); // For simplicity, sending to admin for now - can be treasury management logic later.

        // Refund any extra payment
        if (msg.value > price) {
            payable(msg.sender).transfer(msg.value - price);
        }

        emit ArtSold(_artId, msg.sender, price);
    }


    // --- 5. Membership & Governance ---

